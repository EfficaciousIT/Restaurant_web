@model SmartRestaurant.Model.DashBoardModel

<table class="table table-striped table-bordered">
    <tr>
        <th width="10%">
            @Html.DisplayNameFor(model => model.Order_id)
        </th>
        <th width="80%">
            @Html.DisplayNameFor(model => model.Table_Name)
        </th>
        <th width="10%">
            @Html.DisplayNameFor(model => model.Total)
        </th>
    </tr>
    @foreach (var item in Model.BillDetail)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Order_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Table_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
        </tr>
    }
</table>