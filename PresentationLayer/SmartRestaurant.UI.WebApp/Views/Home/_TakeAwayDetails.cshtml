@model SmartRestaurant.Model.DashBoardModel

<table class="table table-striped table-bordered">
    <tr>
        <th width="10%">
            @Html.DisplayNameFor(model => model.Order_id)
        </th>
        <th width="35%">
            @Html.DisplayNameFor(model => model.First_Name)
        </th>
        <th width="10%">
            @Html.DisplayNameFor(model => model.Mobile_No)
        </th>
        <th width="35%">
            @Html.DisplayNameFor(model => model.Address_1)
        </th>
        <th width="10%">
            @Html.DisplayNameFor(model => model.Total)
        </th>
    </tr>
    @foreach (var item in Model.TakeAwayDetail)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Order_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.First_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mobile_No)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address_1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Total)
        </td>
    </tr>
    }
</table>